{
    "terminal.integrated.shellArgs.windows": [
        "cd ./src; qsetup"
    ],
    "multiCommand.commands": [
        {
            "command": "multiCommand.compile",
            "sequence": [
                "workbench.action.terminal.clear",
                "workbench.action.files.save",
                {
                    "command": "runInTerminal.run", 
                    "args": {
                        "cmd": "./build.ps1",
                        "match": ".*"
                    }
                }
            ]
        }
    ],
    "C_Cpp.errorSquiggles": "Disabled"
}